# Windsorize wages
m_qtiles = quantile(D$m_wage,c(0.02,0.98),na.rm=TRUE)
f_qtiles = quantile(D$f_wage,c(0.02,0.98),na.rm=TRUE)
D$m_wage[D$m_wage<m_qtiles[1]] <- NA
D$m_wage[D$m_wage>m_qtiles[2]] <- NA
D$f_wage[D$f_wage<f_qtiles[1]] <- NA
D$f_wage[D$f_wage>f_qtiles[2]] <- NA
D$m_ed <- 1
D$m_ed[D$m_hgc>=12] <- 2
D$m_ed[D$m_hgc>=16] <- 3
